package routines;


public class Image {

	public String uri;

	
	
	public String getUri() {
		return uri;
	}
	public void setUri(String uri) {
		this.uri = uri;
	}
	public String getUriXML() {
		return "              <uri>"+this.uri+"</uri>";
	}
	


	public String toXML() {
		return 
          "            <image>\n"+
            this.getUriXML()+"\n"+
          "            </image>\n";
	}
	
	
}
